.container{min-height:100vh;background:#f6f7fb}.container>.mask{width:100vw;height:100vh;position:fixed;top:0;left:0;background:rgba(0,0,0,.5);z-index:98}.container>.popup{width:76vw;background:#fff;border-radius:1.33vw;position:fixed;top:50%;left:50%;transform:translate(-50%,-50%);z-index:99;padding:2.67vw 4vw}.container>.popup>.title{display:flex;align-items:center;justify-content:space-between;font-weight:700;font-size:4.27vw;color:#333;margin-bottom:4vw}.container>.popup>image{margin:0 auto;width:26.4vw;height:32.27vw;display:block}.container>.popup>.text{text-align:center;margin:4.53vw 0 2.67vw}.container>.popup>.confirm{width:78.67vw;height:8vw;background:#f97e39;border-radius:4vw;line-height:8vw;text-align:center;font-weight:700;font-size:3.47vw;color:#fff}.container>.content{padding:4vw}.container>.content>.btn{width:80vw;height:8vw;background:#f97e39;border-radius:4vw;text-align:center;line-height:8vw;font-weight:700;font-size:3.47vw;color:#fff;margin:5.33vw auto}.container>.content>.form{background:#fff;border-radius:1.33vw;padding:2.67vw}.container>.content>.form>.info{display:flex;align-items:center;margin-bottom:8vw;font-weight:500;font-size:3.2vw;color:#333}.container>.content>.form>.info>.label{width:20vw;display:flex;align-items:center}.container>.content>.form>.info>.label:before{content:"*";font-weight:500;font-size:3.2vw;color:#ff8177}.container>.content>.form>.card{display:flex;align-items:center;justify-content:space-between;margin-bottom:5.33vw}.container>.content>.form>.card>.item{width:40.8vw;height:22.93vw;border-radius:1.33vw;border:1px dotted #999999;display:flex;align-items:center;justify-content:center;flex-direction:column;font-weight:700;font-size:3.2vw;color:#999}
