.container > .mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 98;
}
.container > .notice_popup {
  position: fixed;
  width: 76.26vw;
  padding: 2.8vw 3.87vw 4.4vw;
  background: #FFFFFF;
  border-radius: 1.33vw;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 99;
}
.container > .notice_popup .html_content {
  word-break: break-all;
}
.container > .notice_popup > .head {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 2.8vw;
  font-weight: bold;
  font-size: 4.27vw;
  color: #333333;
}
.container .content {
  padding: 0 4vw;
}
.container .content > .list > .item {
  background: #FFFFFF;
  border-radius: 1.33vw;
  position: relative;
  margin-top: 2.67vw;
}
.container .content > .list > .item > .feature {
  height: 7vw;
  padding: 0 2.67vw;
  margin-top: 2.67vw;
  border-top: solid 1px #999999;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.container .content > .list > .item > .feature > .left {
  width: 80vw;
  display: flex;
  align-items: center;
  font-size: 3.2vw;
  color: #333333;
}
.container .content > .list > .item > .feature > .left > .pro_feature {
  font-weight: bold;
  color: #109181;
  margin-right: 2vw;
}
.container .content > .list > .item > .option {
  padding-left: 2.67vw;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.container .content > .list > .item > .option > .num {
  background: #F6F7FB;
  border-radius: 0.8vw;
  padding: 0.67vw 1.33vw;
  margin-right: 1.33vw;
  font-size: 2.93vw;
  color: #F97E39;
}
.container .content > .list > .item > .detail {
  padding: 2.67vw;
  display: flex;
  align-items: center;
  font-size: 3.2vw;
}
.container .content > .list > .item > .detail > .max {
  color: #666666;
}
.container .content > .list > .item > .detail > .decimal {
  width: 30vw;
  text-align: center;
  color: #109181;
}
.container .content > .list > .item > .detail > .annualized {
  width: 10.67vw;
  height: 5.33vw;
  border-radius: 0.8vw;
  border: 1px solid #F97E39;
  font-weight: bold;
  color: #F97E39;
  line-height: 5.33vw;
  text-align: center;
}
.container .content > .list > .item > .title {
  height: 5.33vw;
  padding: 1.33vw 2.67vw 0;
  display: flex;
  align-items: center;
  font-weight: bold;
  font-size: 3.73vw;
  color: #333333;
}
.container .content > .list > .item > .title > .classify {
  width: 16vw;
  height: 100%;
  border-radius: 1.33vw;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 2.67vw;
  font-size: 3.2vw;
  color: #FFFFFF;
}
.container .content > .list > .item > .type {
  position: absolute;
  right: 2.67vw;
  top: 0;
  width: 18.67vw;
  height: 7.6vw;
  background: #109181;
  border-radius: 0 0 1.33vw 1.33vw;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: bold;
  font-size: 3.2vw;
  color: #FFFFFF;
}
.container .content .screen {
  width: -webkit-fit-content;
  width: fit-content;
  height: 7vw;
  display: flex;
  align-items: center;
}
.container .content .screen > .item {
  white-space: nowrap;
  padding: 1vw 3vw;
  background: #FFFFFF;
  border-radius: 2.67vw;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 4vw;
  font-size: 3.2vw;
  color: #666666;
}
.container .content .screen > .active {
  color: #FFFFFF;
  background: #F97E39;
}
.container .content .notice {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 3.33vw 2.67vw;
  background: #FFFFFF;
  border-radius: 1.33vw;
}
.container .content .notice .type {
  width: 10.67vw;
  height: 5.33vw;
  border-radius: 0.8vw;
  border: 1px solid #F97E39;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 3.47vw;
  color: #F97E39;
}
.container .content .notice .title {
  flex: 1;
  padding: 0 2.8vw;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 3.47vw;
  color: #333333;
}
.container .content .district {
  height: 18.13vw;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background: #FFFFFF;
  border-radius: 1.33vw;
  margin: 2.67vw 0;
}
.container .content .district > .item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 3.2vw;
  color: #333333;
  font-weight: bold;
}
.container .content .district > .item > image {
  width: 6.4vw;
  height: 6.4vw;
  margin-bottom: 1.07vw;
}
.container .content .banner {
  height: 31.87vw;
}
.container .content .banner image {
  width: 100%;
  height: 100%;
  overflow: hidden;
  border-radius: 1.33vw;
}
.container > .head {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  left: 0;
  width: 100%;
  padding-left: 4vw;
  display: flex;
  align-items: center;
}
.container > .head > .search {
  display: flex;
  align-items: center;
  width: 60vw;
  padding: 0 2.67vw;
  height: 8vw;
  background: #FFFFFF;
  border-radius: 4.53vw;
}
.container > .bg {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
}
.container > .bg > image {
  width: 100%;
  height: 100%;
}
.inputClass {
  font-size: 3.2vw;
  color: #999999;
}